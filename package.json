{
  "name": "serverless-twitter-image-slack",
  "version": "1.0.0",
  "description": "Search tweet with image from Twitter and post to Slack channel",
  "main": "index.js",
  "scripts": {
    "deploy": "sls deploy -v",
    "logs": "sls logs -f tweet_cat -t",
    "invoke": "sls invoke -f tweet_cat --path ./event.json",
